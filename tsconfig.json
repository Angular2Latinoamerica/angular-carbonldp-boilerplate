{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es5",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"noImplicitAny": false,
		"moduleResolution": "node",
		"baseUrl": ".",
		"paths": {
			"app/*": [
				"src/app/*"
			]
		}
	},
	"exclude": [
		"dist",
		"node_modules"
	],
	"angularCompilerOptions": {
		"genDir": "compiled",
		"skipMetadataEmit": true,
		"typeChecking": true
	}
}